{
  "compilerOptions": {
    "target": "es2017",
    "module": "esnext",
    "moduleResolution": "bundler",
    "lib": [
      "es2017",
      "dom",
      "dom.iterable"
    ],

    "noEmit": false,
    "sourceMap": true,
    "declaration": false,
    "declarationMap": false,
    "tsBuildInfoFile": ".tsbuildinfo",

    "noCheck": false,
    "strict": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "experimentalDecorators": true
  }
}
